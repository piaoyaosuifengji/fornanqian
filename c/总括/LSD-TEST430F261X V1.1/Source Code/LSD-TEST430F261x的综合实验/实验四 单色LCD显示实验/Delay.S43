        NAME    Delay
        PUBLIC  Delay 
        RSEG    My_code
Delay:
        PUSHX      R4
        PUSHX      R5
        MOVX       #100, R4           
DEC_R4:
        MOVX       #1000, R5
DEC_R5:
        DECX       R5
        JNZ       DEC_R5
        DECX       R4
        NOP
        JNZ       DEC_R4
        POPX       R5
        POPX       R4
        RET
        END