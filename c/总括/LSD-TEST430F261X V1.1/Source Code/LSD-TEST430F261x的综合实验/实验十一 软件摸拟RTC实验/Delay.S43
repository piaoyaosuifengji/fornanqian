        NAME    Delay
        PUBLIC  Delay 
        RSEG    CODE
Delay:
        PUSH      R4
        PUSH      R5
        MOV       #120, R4           
DEC_R4:
        MOV       #1000, R5
DEC_R5:
        DEC       R5
        JNZ       DEC_R5
        DEC       R4
        NOP
        JNZ       DEC_R4
        POP       R5
        POP       R4
        RET
        END